5) Create Product:
Url: http://localhost:8080/products/create

Request:

{
        "city": {
            "id": 1,
            "name": "Medellin"
        },
        "politic": {
            "id": 1,
            "rule": {
                "title": "r.title1",
                "description": "r.description1"
            },
            "security": {
                "title": "s.title1",
                "description": "s.description1"
            },
            "cancellation": {
                "title": "c.title1",
                "description": "c.description1"
            }
        },
        "images": [
            {
                "title": "image1",
                "url": "rule1"
            },
            {
                "title": "image2",
                "url": "rule2"
            }
        ],
        "category": {
            "title": "title1",
            "description": "description1",
            "imageUrl": "image_url1"
        },
        "characteristic": {
            "kitchen": true,
            "tv": true,
            "conditionAir": true,
            "petsAllowed": true,
            "freeParking": true,
            "pool": true,
            "wifi": true
        },
        "title": "product",
        "description": "description",
        "availability": "availability"
}

response: 

{
    "status": 200,
    "message": "Product created successfully",
    "data": null
}
